{
  "family": "newai-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "newai-container",
      "image": "<image-uri>", // This will be replaced by GitHub Actions with the actual image URI
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "FLASK_ENV",
          "value": "production"
        },
        {
          "name": "FLASK_DEBUG",
          "value": "false"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::730335202608:role/ecsTaskExecutionRole", // Replace with your actual role ARN
  "taskRoleArn": "arn:aws:iam::730335202608:role/ecsTaskExecutionRole" // Replace with your actual role ARN
}
